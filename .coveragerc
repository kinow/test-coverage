[run]

branch=True
cover_pylib=False
concurrency=thread
#data_file=.coverage
disable_warnings=
debug=
#    callers
#    dataio
#    multiprocess
#    dataio
#    dataop
#    pid
#    plugin
#    process
#    sys
#    trace
# Include can be used only if source is not used!
#include
note=
omit=
    */sample/tests*
#parallel = True
plugins=
source=
    ./sample
timid = False


[report]

exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    def parse_args
fail_under=0
ignore_errors = False
include=
omit=
    */sample/tests*
partial_branches=
precision=2
show_missing=False
skip_covered=False
sort=Name


[html]

directory=htmlcov
extra_css=
title=


[xml]

output=coverage.xml
package_depth=99